    "react-md": "^1.12.1",
    "react-router-dom": "^5.0.1",
    "reactn": "^2.1.4"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "app-manifest-webpack-plugin": "^1.2.0",
    "autoprefixer": "^9.5.1",
